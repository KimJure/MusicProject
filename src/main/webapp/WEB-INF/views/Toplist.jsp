    var strHours = d.getHours();

    //alert(strHours);

	for ( i=0; i< 25;i++ ) {
		strTime = (i < 10 ? '0' + i : i);

		if (strTime > strHours) {
			$(".i[alt='" + strTime +  "']").addClass("disabled");
			$(".i[alt='" + strTime +  "']").attr("href", "#");
			$(".i[alt='" + strTime +  "']").attr("click", "return false;");
		} else {
			$(".i[alt='" + strTime +  "']").attr("href", $("#strLink").val() + "&hh=" + strTime);
		};
		//$(".i[alt='" + strTime +  "']").attr("href", "#" + strTime);
		//$(".i[alt='" + strTime +  "']").html(strTime);
		$(".i[alt='" + strTime +  "']").text(strTime + ':00');

	};

    var TimeScroll_layer = function(o){
        this.hook = $(o);
    };


    TimeScroll_layer.prototype = {
        _close : function(){
            var that = this;
            that.hook.animate({ 'opacity' : 0 },'fast', function(){
                $(this).css({ 'display' : 'none' });
                that._flag = 'close';
            });

            Object.extend(timeScrollBar.scrollbarStyle, {
                display:'none'
            });
            timeScrollBar.reset();
        },
        _load : function(tar){
            var that = this;
            if (that._flag == 'open') {
                that._close();
                return false;
            }
            that.hook.css({
                'display' : 'block'
                , 'opacity' : 0
                , 'position' : 'absolute'
                , 'top' : '30px'
                , 'left' : $(tar).offset().left - 220
            });
            that.hook.animate({
                opacity : 1
            }, 'fast', function(){
                that._flag = 'open'
            });

            Object.extend(timeScrollBar.scrollbarStyle, {
                display:'block',
                zIndex:81
            });
            timeScrollBar.reset();
        }
    };

    window.TimeScroll_layer = TimeScroll_layer;

})();

